#!/bin/bash

if [[ $# -eq 1 ]]; then
    selected=$1
else
    selected=$({ \
        find ~/dev/personal ~/dev/sosha/lightspeed -mindepth 1 -maxdepth 1 -type d; \
        echo ~/dev/sosha/sosha-conversion-tracking; \
        echo ~/.ioansx; \
    } | fzf)
fi

if [[ -z $selected ]]; then
    exit 0
fi

echo "$selected"
